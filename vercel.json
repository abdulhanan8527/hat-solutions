{
  "version": 2,
  "builds": [
    {
      "src": "hat_solutions/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "maxLambdaSize": "15mb",
        "installCommand": "pip install -r requirements.txt && python manage.py collectstatic --noinput && python manage.py upload_static"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "hat_solutions/wsgi.py"
    }
  ]
}